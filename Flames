print("Enter the names to check the FLAMES")
a=input("Enter boy name:")
b=input("Enter girl name:")
a1=list(a)
b1=list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if (a1[i]==b1[j]):
            a1[i]='1'
            b1[j]='1'

c=0
for i in a1:
    if (i !='1'):
      c+=1
for i in b1:
    if i !='1':
     c+=1

f=0
n=list("FLAMES")
for i in range(5):
    f=(f+(c-1))%len(n)
    n.pop(f)
print("Flames:", a,'and ',b,":",*n)
